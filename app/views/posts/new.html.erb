<h1>New Post</h1>
<%= form_with model: @post do |form| %>
<p>
    <%= form.label "Title"%>
    <%= form.text_field "title" %>
</p>
<p>
    <%= form.label "Description"%>
    <%= form.text_field "description" %>
</p>
<p>
    <%= form.label "Date Posted" %>
    <%= form.date_field "posted_on" %>
</p>
  <%= form.submit %>
<% end %>